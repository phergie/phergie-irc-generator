{
    "minimum-stability": "dev",
    "name": "phergie/phergie-irc-generator",
    "description": "Generator for messages conforming to the IRC protocol",
    "keywords": [ "irc", "generator" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-generator/issues",
        "source": "https://github.com/phergie/phergie-irc-generator"
    },
    "require": {
	"php": "^7.0||^5.6"
    },
    "require-dev": {
        "phergie/phergie-irc-bot-react-development": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/phergie/phergie-irc-generator"
        }
    ]
}
